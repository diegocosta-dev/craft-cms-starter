{#

Picture Block component

Usage:

{{ include('_partials/entry/_picture', { Parameters }) }}

Parameters:

- entry: The block object

Picture Block object model:

{% set entry = {
  image: Asset,
} %}

#}
{# prettier-ignore-start #}
{% if entry is defined and entry and entry.image is defined and entry.image %}
  {{ include('_partials/_img', {
    imgAsset: entry.image|first,
    classes: 'w-full object-cover'
  }) }}
{% endif %}